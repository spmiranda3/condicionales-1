numeros = []
for i in range(3):
    numero = float(input("Introduce el nÃºmero #{}: ".format(i + 1)))
    numeros.append(numero)
mayor = numeros[0]
for numero in numeros:
    if numero > mayor:
        mayor = numero
print("Mayor:", mayor)
menor = numeros[0]
for numero in numeros:
    if numero < menor:
        menor = numero
print("Menor:", menor)
